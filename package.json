{
  "name": "registration-assistant",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "build": "webpack --config config/prod.webpack.config.js",
    "build:prod": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "deploy": "npm-run-all build:prod lint test",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "nightly": "npm run deploy",
    "prod": "NODE_ENV=production webpack-dev-server --config config/dev.webpack.config.js",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "NODE_ENV=development webpack-dev-server --config config/dev.webpack.config.js",
    "test": "jest --verbose",
    "translations:compile": "npm run build; node src/Utilities/mergeMessages.js",
    "travis:build": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "travis:verify": "npm-run-all travis:build lint test",
    "verify": "npm-run-all build lint test"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/stories/*"
    ],
    "coverageDirectory": "./coverage/",
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "roots": [
      "<rootDir>/src/"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ]
  },
  "dependencies": {
    "@babel/runtime": "7.11.2",
    "@data-driven-forms/pf4-component-mapper": "2.8.9",
    "@data-driven-forms/react-form-renderer": "2.8.9",
    "@patternfly/patternfly": "4.31.6",
    "@patternfly/react-core": "4.40.4",
    "@patternfly/react-icons": "4.7.2",
    "@patternfly/react-table": "4.15.5",
    "@redhat-cloud-services/frontend-components": "2.3.14",
    "@redhat-cloud-services/frontend-components-inventory-insights": "2.5.6",
    "@redhat-cloud-services/frontend-components-translations": "2.1.0",
    "@redhat-cloud-services/frontend-components-utilities": "2.1.4",
    "classnames": "2.2.6",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-intl": "5.6.5",
    "react-redux": "7.2.1",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-promise-middleware": "6.1.2",
    "seamless-immutable": "7.1.4"
  },
  "devDependencies": {
    "@babel/core": "7.11.1",
    "@babel/plugin-proposal-object-rest-spread": "7.11.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.11.0",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-flow": "7.10.4",
    "@babel/preset-react": "7.10.4",
    "@redhat-cloud-services/frontend-components-config": "2.1.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.3.0",
    "babel-plugin-dual-import": "1.2.1",
    "babel-plugin-react-intl": "7.9.2",
    "babel-plugin-transform-imports": "2.0.0",
    "cross-fetch": "3.0.5",
    "css-loader": "3.6.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.3",
    "enzyme-to-json": "3.5.0",
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.4.0",
    "node-sass": "4.14.1",
    "npm-run-all": "4.1.5",
    "postcss": "7.0.32",
    "prop-types": "15.7.2",
    "stylelint": "13.6.1",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-scss": "3.18.0",
    "typescript": "3.9.7",
    "webpack": "4.44.1",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.12"
  },
  "insights": {
    "appname": "registration"
  }
}
